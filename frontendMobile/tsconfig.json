{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "./@types",
    "./app/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
